{
  "name": "expense-report-ai",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ginxo/expense-report-ai.git"
  },
  "keywords": [
    "expenses",
    "machine learning",
    "ML",
    "artificial inteligence",
    "ai",
    "bank",
    "report",
    "antd",
    "reactjs"
  ],
  "dependencies": {
    "@auth0/auth0-react": "^2.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.4",
    "@types/react-dom": "^18.2.4",
    "antd": "^5.5.1",
    "axios": "^1.4.0",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-router-dom": "^6.2.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-react-app": "^7.0.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "flow": "flow",
    "pre:push": "yarn",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint -- --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}